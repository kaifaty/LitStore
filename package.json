{
  "name": "lit-store",
  "version": "1.1.1",
  "description": "Extry tiny state manager for LitElement",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "typesVersions": {
    "<3.8": {
      "*": [
        "ts3.4/*"
      ]
    }
  },
  "scripts": {
    "dev": "parcel demo/index.html -d ./demo/dist",
    "build": "tsc"
  },
  
  "files": [
    "/demo/",
    "/src/",
    "/dist/"
  ],
  "keywords": [
    "state",
    "store",
    "management",
    "lit-element",
    "LitElement",
    "app",
    "shared",
    "global",
    "observer",
    "observable"
  ],
  "homepage": "https://github.com/kaifaty/LitStore#readme",
  "bugs": {
    "url": "https://github.com/kaifaty/LitStore/issues"
  },
  "author": "kaifaty",
  "license": "MIT",
  
  "devDependencies": {
    "lit-element": "latest",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
